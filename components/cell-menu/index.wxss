.container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    height: 96rpx;
}

.container-left {
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-left: 32rpx;
    margin-right: 32rpx;
}

.txt-menu {
    color: #3e3e3e;
    font-size: 32rpx;
}

.txt-content {
    color: #6F6F6F;
    font-size: 28rpx;
    margin-right: 8rpx;
}

.img-icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}

.icon-right {
    width: 32rpx;
    height: 32rpx;
}
