.navbar {
    width: 100%;
    flex-shrink: 0;
    display: flex;
    flex-direction: column;
    position: relative;
}

.lock-bar {
    width: 100%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
    flex-shrink: 0;
    background: white;
    display: flex;
    flex-direction: column;
}

.topbar {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.view-search{
    flex: 1;
    margin-right: 16rpx;
    margin-left: 8rpx;
    border-radius: 40rpx;
    background: #f7f7f7;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    border: solid 0.5px #f5f5f5;
}

.input-area{
    font-size: 28rpx;
    text-align: left;
}