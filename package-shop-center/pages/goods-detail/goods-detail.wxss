swiper {
    display: flex;
    width: 100%;
    height: 372rpx;
}

swiper image {
    width: 100%;
    height: 100%;
}

.container-capsure {
    display: flex;
    flex-direction: row;
    border-radius: 40rpx;
    background-color: white;
    border: solid 1px #c7d0ce;
    box-sizing: border-box;
    opacity: 0.6;
    justify-content: center;
    align-items: center;
    padding-left: 16rpx;
    padding-right: 16rpx;
}

.container-main {
    height: 372rpx;
}

.container-white {
    height: 112rpx;
}

.container-half {
    width: 319rpx;
}

.container-btn {
    width: 319rpx;
    height: 84rpx;
    border-radius: 16rpx;
    background: red;
}
