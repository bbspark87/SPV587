.container-top {
    display: flex;
    flex-direction: row;
    width: 100%;
    justify-content: space-between;
    margin-top: 32rpx;
    align-items: center;
}

.container-bottom {
    display: flex;
    flex-direction: row;
    width: 100%;
    justify-content: center;
    align-items: center;
    margin-bottom: 48rpx;
    position: fixed;
    bottom: 0;
}

.btn-login {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 96rpx;
    background: aqua;
}

.container-left-btn {
    display: flex;
    flex-direction: row;
    margin-left: 32rpx;
    justify-content: center;
    align-items: center;
    flex: 1;
    height: 96rpx;
    border-radius: 16rpx;
    border: solid 1px var(--theColor);
    background: white;
}

.container-ritht-btn {
    flex: 1;
    display: flex;
    margin-left: 16rpx;
    margin-right: 32rpx;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 96rpx;
    border-radius: 16rpx;
    background: var(--theColor);
}

.txt-title {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-left: 32rpx;
}

.txt-gonglve {
    background: white;
    border: solid 1px var(--borderColor);
    border-radius: 8rpx;
    justify-content: center;
    padding: 8rpx;
    align-items: center;
    margin-right: 32rpx;
}

