.container-item {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-top: 32rpx;
}

.container-list {
    width: calc(100% - 64rpx);
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background: white;
    border: solid 1rpx #F3F3F3;
    border-radius: 16rpx;
    box-shadow: 1px 2rpx 5rpx #E5E5E5;
}

.img-rank-num {
    width: 48rpx;
    height: 48rpx;
    margin-left: 32rpx;
}

.img-rank {
    width: 128rpx;
    height: 52rpx;
    margin: 32rpx;
}

.txt-nickname {
    flex: 1;
    display: flex;
    flex-direction: row;
    margin-left: 32rpx;
    margin-right: 32rpx;
}

.img-avatar {
    width: 72rpx;
    height: 72rpx;
    border-radius: 64rpx;
    margin: 32rpx;
}

.container-bottom {
    display: flex;
    flex-direction: row;
    width: 100%;
    justify-content: center;
    align-items: center;
    margin-bottom: 0rpx;
    background-color: white;
    border: solid 1rpx #dbdbdb;
}

.container-self {
    width: calc(100% - 64rpx);
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background: white;
}
