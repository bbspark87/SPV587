/**app.wxss**/
.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fafafa;
}

.blank-flex {
  display: flex;
  flex: 1;
}

.menu-text-title {
  padding-left: 30rpx;
  align-items: center;
  width: 120rpx;
  color: #535353;
  justify-content: center;
  font-size: 32rpx;
}

.menu-text {
  align-items: center;
  color: #3b3b3b;
  justify-content: center;
  font-size: 32rpx;
}

.devider-line {
  background: #f5f5f5;
  width: calc(100% - 0rpx);
  height: 1px;
}

.full-devider-line {
  background: #f5f5f5;
  width: 100%;
  height: 1px;
}

.icon-rignt {
  margin-right: 30rpx;
}

.main-button {
  color: aqua;
}