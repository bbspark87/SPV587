@import "style/layout.wxss";
@import "style/dimen.wxss";
@import "style/color.wxss";

page {
    background: #f7f7f7;
    --theColor: #6469DB;
    --textColor: #141414;
    --borderColor: #e5e5e5;
    --dividerColor: #f5f6f8;

    --cell-text-color: #141414;
    --cell-font-size: 32rpx;
}

.half-divider {
    margin-left: 32rpx;
    margin-right: 32rpx;
    height: 1px;
    background: var(--dividerColor);
}

.full-divider {
    height: 1px;
    background: var(--dividerColor);
}

.blank-view {
    width: 100%;
    height: 32rpx;
}

.blank-view-small {
    width: 100%;
    height: 16rpx;
}

.container-background {
    width: 100%;
    height: 100%;
}

.txt-tag {
    background: white;
    border-radius: 4rpx;
    border: solid 1rpx #e5e5e5;
    margin-right: 16rpx;
}

